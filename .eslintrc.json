{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 15,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
        "no-console": "off",
        "func-names": "off",
        "no-underscore-dangle": "off"
    }
}
